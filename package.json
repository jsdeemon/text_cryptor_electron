{
  "name": "image-shrinker",
  "productName": "Text Cryptor",
  "version": "1.0.0",
  "description": "App to make images smaller",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . --overwrite --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=false --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Text Cryptor\"",    
    "package-linux": "electron-packager . --overwrite --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=false --out=release-builds"
  },
  "author": "Dmitry",
  "license": "ISC",
  "devDependencies": {
    "electron": "^16.0.6",
    "electron-packager": "^15.4.0"
  }
}
